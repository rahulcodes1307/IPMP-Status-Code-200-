    #include <stdio.h>
    int fibo(int);
    int main(){
    	int num;
    	scanf("%d", &num);              			// Reading input from STDIN 
    	int c;
    	c=fibo(num);
    	printf("%d",c);
    	    
    }
    int fibo(int n){
    	if(n==0 || n==1) return 0;
    	if(n==2) return 1;
    	else return fibo(n-1)+fibo(n-2);
    }
